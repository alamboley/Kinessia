/** * Tutoriel "Les jeux à base de tuiles * "http://ressources.mediabox.fr/tutoriaux/flashplatform/jeux/jeux_a_base_de_tuiles" *  * Classe "Objet" * */package kinessia.pacman {	import flash.display.MovieClip; 	public class Objet extends MovieClip	{		/**		 * Position de l'objet sur la carte.		 */		private var _positionX:int;		private var _positionY:int; 		/**		 * Type d'objet.		 */		private var _type:String; 		/**		 * Carte dans laquelle l'objet se trouve.		 */		private var _carte:Carte; 		/**		 * Constructor		 */		public function Objet ()		{		}				/**		 * Méthode executée pour détruire l'instance.		 */		public function dispose ():void		{			_type = null;						delete this;		} 		/**		 * Getters		 */		public function get positionX ( ):int { return _positionX };		public function get positionY ( ):int { return _positionY };		public function get type ( ):String { return _type };		public function get carte ( ):Carte { return _carte }; 		/**		 * Setters		 */		public function set positionX ( $positionX:int ):void { _positionX = $positionX };		public function set positionY ( $positionY:int ):void { _positionY = $positionY };		public function set type ( $type:String ):void { _type = $type };		public function set carte ( $carte:Carte ):void { _carte = $carte };	}}